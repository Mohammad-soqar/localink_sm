  <string>We need permission to save photos to your photo library</string>

  <!-- Camera & Microphone Permissions -->
  <key>NSCameraUsageDescription</key>
  <string>We need access to your camera for capturing photos and videos</string>
  <key>NSMicrophoneUsageDescription</key>
  <string>We need access to your microphone for capturing audio</string>

  <!-- If your app needs to read the user's media or write to the library -->
  <key>NSAppleMusicUsageDescription</key>
  <string>We need access to your media library for selecting background music</string>
  
